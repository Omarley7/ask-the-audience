{
  "name": "ask-the-audience-wedding",
  "private": true,
  "scripts": {
    "postinstall": "npm --prefix client install",
    "serve": "node server/index.js",
    "client": "npm --prefix client run dev",
    "dev": "concurrently \"npm run serve\" \"npm run client\"",
    "ci": "yarn install && yarn --cwd client install && yarn --cwd client build"
  },
  "dependencies": {
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.19.2",
    "@supabase/supabase-js": "^2.45.3",
    "qrcode": "^1.5.3",
    "socket.io": "^4.7.5"
  }
}
